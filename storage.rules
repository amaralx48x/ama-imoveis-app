rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {

    // Fotos de agentes
    match /agents/{agentId}/{allPaths=**} {
      // Leitura p√∫blica
      allow read: if true;

      // Escrita apenas para o corretor dono
      allow write: if request.auth != null && request.auth.uid == agentId;
    }
  }
}
